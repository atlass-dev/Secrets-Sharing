@model User

<h1>@Model.Email</h1>

<table>
    <tr>
        <th>
            Название
        </th>
        <th>
            Тип
        </th>
    </tr>
    @foreach(var resource in Model.Resources)
    {
        <tr>            
            <td>
                <a asp-controller="Profile" asp-action="File" asp-route-hash="@resource.Hash">@resource.Name</a>
            </td>
            <td>
                @resource.Type
            </td>
        </tr>
    }
</table>
<a asp-controller="Profile" asp-action="FileLoad">Загрузить файл</a>
